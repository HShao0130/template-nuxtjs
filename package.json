    "vue2-toast": "^2.0.2",
    "vuetify": "^1.2.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.1.0",
